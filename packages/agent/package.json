{
  "name": "@vibepilot/agent",
  "version": "0.1.0",
  "private": true,
  "description": "VibePilot backend agent with PTY sessions, WebSocket server, and WebRTC support",
  "license": "SEE LICENSE IN ../../LICENSE",
  "author": "Your Name",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/vibepilot.git",
    "directory": "packages/agent"
  },
  "type": "module",
  "bin": {
    "vibepilot": "./dist/bin/vibepilot.js"
  },
  "scripts": {
    "postinstall": "node -e \"const fs=require('fs'),p=require('path'),d=p.dirname(require.resolve('node-pty')),h=p.join(d,'prebuilds',process.platform+'-'+process.arch,'spawn-helper');try{fs.chmodSync(h,0o755);console.log('Fixed spawn-helper permissions')}catch{}\"",
    "build": "tsc",
    "dev": "tsx bin/vibepilot.ts serve",
    "start": "node dist/bin/vibepilot.js",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "@vibepilot/protocol": "workspace:*",
    "chokidar": "^4.0.0",
    "chrome-remote-interface": "^0.34.0",
    "commander": "^13.0.0",
    "jose": "^6.1.3",
    "node-datachannel": "^0.11.0",
    "node-pty": "^1.0.0",
    "open": "^10.0.0",
    "pino": "^9.0.0",
    "pino-pretty": "^13.0.0",
    "uuid": "^11.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/chrome-remote-interface": "^0.33.0",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
